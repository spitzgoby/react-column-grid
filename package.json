{
  "name": "swa-react-grid",
  "version": "1.0.0",
  "description": "A simple grid layout system using React",
  "main": "build/index.js",
  "scripts": {
    "clean": "rm -rf node_modules dist",
    "start": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.config.js",
    "test": "jest",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand --watch"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./setupTests.js"
    ],
    "testEnvironment": "jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spitzgoby/swa-react-grid.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spitzgoby/swa-react-grid/issues"
  },
  "homepage": "https://github.com/spitzgoby/swa-react-grid#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.3.1",
    "jsdom": "^18.0.1",
    "jsdom-global": "^3.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-jss": "^10.8.2",
    "react-syntax-highlighter": "^15.4.5",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  },
  "peerDependencies": {
    "classnames": "^2.3.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-jss": "^10.8.2"
  }
}
